LISTEN_HOST=127.0.0.1
LISTEN_PORT=9038

UWSGI_LOGTO="/var/log/eng_education/uwsgi.log"
UWSGI_MASTER=true
UWSGI_PROCESSES=1
UWSGI_THREADS=2
UWSGI_HARAKIRI=180
UWSGI_HARAKIRI_VERBOSE=true
