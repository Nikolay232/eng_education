UWSGI_BUFFER_SIZE=65535
UWSGI_NO_ORPHANS=true
UWSGI_UID=eng_education
UWSGI_GID=eng_education
ENG_EDUCATION_SETTINGS_OVERRIDE_PATH=/etc/eng_education/settings.py
